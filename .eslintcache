OM ALL THE INPUT FIELDS\r\n  const onChangeHandler = event => {\r\n    event.preventDefault();\r\n    const value = event.target.value;\r\n    const name = event.target.name;\r\n    setField({\r\n      ...field,\r\n      [event.target.name]: value\r\n    });\r\n    // REGULAR EXPRESSIONS TO CHECK NUMERIC FIELD AND EMAIL FIELD\r\n    const emailRegex = /^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\r\n    const passwordRegex = /[a-z]/i;\r\n\r\n    let errors = errorObject;\r\n    switch (name) {\r\n      case \"email\":\r\n        errors.emailError = emailRegex.test(value) ? \"\" : \"Email is not valid\";\r\n        errors.emailValid = emailRegex.test(value) ? true : false;\r\n        break;\r\n      case \"password\":\r\n        errors.passwordError = passwordRegex.test(value)\r\n          ? \"\"\r\n          : \"Password is not valid\";\r\n        errors.passwordValid = passwordRegex.test(value) ? true : false;\r\n        break;\r\n      default:\r\n        break;\r\n    }\r\n    setError({ ...errors });\r\n  };\r\n\r\n  const loginHandler = async event => {\r\n    event.preventDefault();\r\n    setLoading(true);\r\n    await dispatch(authAction.Login(field.email, field.password));\r\n    setLoading(false);\r\n    console.log(props.state.auth)\r\n    // history.replace(from);\r\n  };\r\n\r\n  // IF NO ERROR FROM THE INPUT FIELDS => isEnabled is TRUE\r\n  const isEnabled = errorObject.emailValid && errorObject.passwordValid;\r\n\r\n  return (\r\n    <Form>\r\n      <h2>LOGIN</h2>\r\n      <Input\r\n        label='Username'\r\n        name='email'\r\n        type='email'\r\n        onChange={onChangeHandler}\r\n        value={field.email}\r\n        setField\r\n      />\r\n      {/* ------------------ERROR MESSAGE AREA------------------------ */}\r\n      {!errorObject.emailValid ? <Error>{errorObject.emailError}</Error> : \"\"}\r\n      <Input\r\n        label='Password'\r\n        name='password'\r\n        type='password'\r\n        onChange={onChangeHandler}\r\n        value={field.password}\r\n      />\r\n      {/* ------------------ERROR MESSAGE AREA------------------------ */}\r\n      {!errorObject.passwordValid ? (\r\n        <Error>{errorObject.passwordError}</Error>\r\n      ) : (\r\n        \"\"\r\n      )}\r\n      {props.state.auth.logInFailed ? (\r\n        <Error>{props.state.auth.logInError.message}</Error>\r\n      ) : null}\r\n      {isLoading ? (\r\n        <ReactLoading\r\n          className='spinner'\r\n          type='spin'\r\n          color={colors.lightBlue}\r\n          height='40px'\r\n          width='40px'\r\n        />\r\n      ) : (\r\n        <Button isDisabled={!isEnabled}>\r\n          <button onClick={loginHandler} disabled={isEnabled ? false : true}>\r\n            Login\r\n          </button>\r\n        </Button>\r\n      )}\r\n    </Form>\r\n  );\r\n};\r\n\r\nconst mapStateToProps = state => {\r\n  return {\r\n    state: state\r\n  };\r\n};\r\n\r\nexport default connect(mapStateToProps)(Login);\r\n","C:\\Users\\konichar\\Documents\\Projects\\best-dating-site\\src\\store\\actions\\auth.js",[],"C:\\Users\\konichar\\Documents\\Projects\\best-dating-site\\src\\utility\\styleUtility.js",[],"C:\\Users\\konichar\\Documents\\Projects\\best-dating-site\\src\\components\\Input.jsx",[],"C:\\Users\\konichar\\Documents\\Projects\\best-dating-site\\src\\store\\reducers\\user.js",[],"C:\\Users\\konichar\\Documents\\Projects\\best-dating-site\\src\\store\\actions\\user.js",[],"C:\\Users\\konichar\\Documents\\Projects\\best-dating-site\\src\\views\\SignIn.jsx",["106"],"C:\\Users\\konichar\\Documents\\Projects\\best-dating-site\\src\\views\\About.jsx",[],"C:\\Users\\konichar\\Documents\\Projects\\best-dating-site\\src\\views\\SignOut.jsx",[],"C:\\Users\\konichar\\Documents\\Projects\\best-dating-site\\src\\views\\Home.jsx",[],"C:\\Users\\konichar\\Documents\\Projects\\best-datin